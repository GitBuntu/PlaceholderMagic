{
  "apiEndpoints": {
    "getDatabases": "http://localhost:5242/databases",
    "getSchemas": "http://localhost:5242/databases/{database}/schemas",
    "getTables": "http://localhost:5242/databases/{database}/schemas/{schema}/tables",
    "getColumns": "http://localhost:5242/databases/{database}/schemas/{schema}/tables/{table}/columns"
  },
  "axiosConfig": {
    "baseURL": "http://localhost:5242",
    "headers": {
      "Authorization": "Bearer YOUR_DEV_ACCESS_TOKEN"
    }
  }
}
