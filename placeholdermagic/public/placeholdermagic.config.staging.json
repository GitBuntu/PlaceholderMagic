{
  "apiEndpoints": {
    "getDatabases": "/api/databases",
    "getSchemas": "/api/databases/{database}/schemas",
    "getTables": "/api/databases/{database}/schemas/{schema}/tables",
    "getColumns": "/api/databases/{database}/schemas/{schema}/tables/{table}/columns",
    "getPreviewData": "/api/databases/{database}/schemas/{schema}/tables/{table}/preview"
  },
  "axiosConfig": {
    "baseURL": "https://staging.example.com",
    "headers": {
      "Authorization": "Bearer YOUR_STAGING_ACCESS_TOKEN"
    }
  }
}
